        $(window).resize(function(){
            $(".lazy_gallery").html('');
            clearTimeout(window.resizedFinished);
            window.resizedFinished = setTimeout(function(){
                load_photos();
            }, 250);
        });

        var block = 1;
        var kol = 0;
        function load_photos()
        {
            var max_width = 300;
            var min_width = 140;
            var otstup = 10;
            var prodGrid_box_width = $('.prodGrid_box').width();

            var randomsize_mode = '';
            if($(".randomsize").hasClass('cut'))
            {
                randomsize_mode = 'cut';
            }

            var min_kol_photo = parseInt(prodGrid_box_width / max_width);
            var photo_box_width = min_kol_photo * max_width + (min_kol_photo - 1) * otstup;
            if(photo_box_width < prodGrid_box_width){
                min_kol_photo = min_kol_photo + 1;
            }

            var max_kol_photo = parseInt(prodGrid_box_width / min_width);

            var kol = $('.lazy_gallery .prodGrid_image.rand').length;
            var general_kol = JSON.parse(sessionStorage.getItem('nsData')).length;

            var URL = location.href;

                $.ajax({
                    type: "POST",
                    url: "https://content.nebo.by/template/catalog/personal-related-ajax.php",
                    data: {
                        type:'1',
                        articul:'34272',
                        block: block,
                        kol: kol,
                        min_kol_photo: min_kol_photo,
                        max_kol_photo: max_kol_photo,
                        prodGrid_box_width: prodGrid_box_width,
                        randomsize_mode:randomsize_mode,
                        session_token:'',
                        URL:URL,
                        max_width:max_width,
                        min_width:min_width,
                        otstup:otstup
                    },
                    cache: false,
                    async: false,
                    success: function (data) {
                        if (data != "") {
                            $(".lazy_gallery").append(data);
                            $("#load").hide();
                            inProgress = false;
                            block++;
                            $('.prodGrid_cell').removeClass('last_cell');
                            $('.prodGrid_cell').last().addClass('last_cell');
                            if($('.lazy_gallery .prodGrid_image.rand').length<general_kol){
                                load_photos();
                            }
                        }
                        else {
                            $("#load").hide();
                            inProgress = false;
                            load = false;
                        }
                    }
                });

        }
        function load_ns_data()
        {
            $.ajax({
                type: "POST",
                url: "https://content.nebo.by/template/catalog/personal-related-ajax.php",
                data: { type:'2', articul:'34272'},
                cache: false,
                dataType: "json",
                success: function(data)
                {
                    sessionStorage.setItem('nsData', JSON.stringify(data));
                    if(data != null)
                    {
                        $('.ppRelatedTitle').html('Используется');
                        load_photos();
                    }
                }
            });
        }

        function photo_like(q,w){}

        $(document).ready(function() {
            $(".lazy_gallery").html('');
            load_ns_data();

        });

        $(window).load(function() {


            if($('.ppAltImages-item').length != 0)
            {
                // alert('test2');
                if(!$('.ppOwl_nsCarousel').hasClass('owl_ns-carousel'))
                {

                    // setTimeout(function () {
                    //    alert('test3');
                    location.reload();
                    // },100);
                }
            }
        });
    </script>

